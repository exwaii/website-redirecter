{
    "manifest_version": 2,
    "name": "Website Redirector",
    "version": "1.0",
    "description": "Redirects away from specified websites",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true  
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  